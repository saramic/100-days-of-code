<% if user_signed_in? %>
  signed in as: <%= current_user.email %>
  <br />
  <%= form_for(current_user, url: destroy_user_session_path, method: :delete) do |f| %>
    <%= f.submit(
      "sign out",
    ) %>
  <% end %>
  <br />
  <%= form_for(current_user, url: setup_2fa_home_path) do |f| %>
    <%= f.submit(
      "setup 2FA",
    ) %>
  <% end %>
<% else %>
  <%= link_to(
    "sign up",
    new_user_registration_path,
  ) %>
  <br />
  <%= link_to(
    "login",
    new_user_session_path,
  ) %>
<% end %>